{
  "name": "@bgd-labs/js-utils",
  "version": "1.2.2",
  "description": "common utils for web3 projects",
  "author": "BGS labs (@bgdlabs)",
  "bugs": {
    "url": "https://github.com/bgd-labs/js-utils/issues"
  },
  "homepage": "https://github.com/bgd-labs/js-utils#readme",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "ci:publish": "npm run build && npm publish --access=public"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/browser.cjs",
  "module": "./dist/browser.js",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      },
      "require": {
        "types": "./dist/browser.d.cts",
        "default": "./dist/browser.cjs"
      },
      "import": {
        "types": "./dist/browser.d.ts",
        "default": "./dist/browser.js"
      },
      "types": "./dist/browser.d.cts",
      "default": "./dist/browser.cjs"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.2",
    "typescript": "^5.1.6",
    "viem": "^2.7.15",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@supercharge/promise-pool": "^3.1.1",
    "bs58": "^5.0.0",
    "gray-matter": "^4.0.3",
    "tsx": "^4.7.1"
  },
  "peerDependencies": {
    "viem": "^2.0.3"
  }
}
